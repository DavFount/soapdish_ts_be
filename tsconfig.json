{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "baseUrl": "./dist",
    "paths": {
      "#configs/*": ["src/configs/*"],
      "#models/*": ["src/models/*"],
      "#controllers/*": ["src/controllers/*"],
      "#middleware/*": ["src/middleware/*"],
      "#services/*": ["src/services/*"],
      "#utils/*": ["src/utils/*"]
    },
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
